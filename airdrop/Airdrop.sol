    mapping(uint256 => bytes32) public _merkleRoots;
    mapping(uint256 => bool) public _isPaused;
    mapping(uint256 => mapping(bytes32 => bool)) public _claimedOrFinalized;

    modifier selfExecute {
        require(_msgSender() == address(this), "Caller must be timelock");
        _;
    }

    function initialize(
        string memory name, 
        string memory version, 
        uint256 salt,
        uint256 minDelay,
        address[] memory proposers,
        address[] memory executors
    ) initializer public {
        __TimelockController_init(minDelay, proposers, executors);
        __ReentrancyGuard_init();
        __ERC1155Holder_init();
        __ERC721Holder_init();

        uint256 chainId;
        assembly {
            chainId := chainid()
        }
        domainSeparator = keccak256(abi.encode(
            domainTypeHash,
            keccak256(abi.encodePacked(name)),
            keccak256(abi.encodePacked(version)),
            keccak256(abi.encodePacked(chainId)),
            keccak256(abi.encodePacked(address(this))),
            keccak256(abi.encodePacked(salt))
        ));

        revokeRole(TIMELOCK_ADMIN_ROLE, _msgSender());
    }

    function seedNewAllocations(
        bytes32 merkleRoot, 
        address funder,
        address erc20Address,
        address erc721Address,
        address erc1155Address,
        uint256 totalCoin,
        uint256 totalToken,
        uint256[] memory erc721Ids,
        uint256[] memory erc1155Ids,
        uint256[] memory amounts
    ) public payable selfExecute returns (uint256 tranche)
    {
        if(totalCoin > 0) {
            require(msg.value >= totalCoin, "Not enough coin transfered");
        }

        if(erc20Address != address(0)) { // Transfer ERC20 to contract
            require(totalToken > 0, "Allocation is less than zero");
            IERC20Upgradeable(erc20Address).safeTransferFrom(funder, address(this), totalToken);
        }

        if(erc721Address != address(0)) { // Transfer NFTs(ERC721) to contract
            require(erc721Ids.length > 0, "List of ERC721 tokens is empty");
            for(uint256 i = 0; i < erc721Ids.length; i++) {
                IERC721Upgradeable(erc721Address).safeTransferFrom(funder, address(this), erc721Ids[i]);
            }
        }

        if(erc1155Address != address(0)) { // Transfer NFTs(ERC1155) to contract
            require(erc1155Ids.length > 0, "List of ERC1155 tokens is empty");
            require(erc1155Ids.length == amounts.length, "Length of amounts is invalid");
            IERC1155Upgradeable(erc1155Address).safeBatchTransferFrom(funder, address(this), erc1155Ids, amounts, bytes(""));
        }

        tranche = _tranches;
        _merkleRoots[tranche] = merkleRoot;

        _tranches = _tranches + 1;

        emit TrancheAdded(tranche, merkleRoot, totalCoin, totalToken, erc721Ids, erc1155Ids, amounts);
    }

    function expireTranche(uint256 tranche)
        public
        selfExecute
    {
        _merkleRoots[tranche] = bytes32(0);
        emit TrancheExpried(tranche);
    }

    function pauseTranche(uint256 tranche)
        public
        selfExecute
    {
        _isPaused[tranche] = true;
        emit TranchePaused(tranche);
    }

    function claimNativeCoin(
        uint256 tranche,
        address receiver,
        uint256 airdropAmount,
        bytes memory signature,
        bytes32[] memory merkleProof
    ) public nonReentrant {
        require(tranche < _tranches, "Airdrop has not open this tranche");
        require(!_isPaused[tranche], "Aridrop is under maintaining mode in this tranche");

        bytes32 hashToSign = _hashAirdropNativeCoin(receiver, airdropAmount);

        require(_verifyReceiver(receiver, tranche, hashToSign, signature), "Invalid signature");
        require(_verifyClaim(tranche, hashToSign, merkleProof), "Incorrect merkle proof");

        _claimedOrFinalized[tranche][hashToSign] = true;

        _disburseNativeCoin(receiver, airdropAmount);

        emit ClaimedNativeCoin(receiver, tranche, airdropAmount);
    }

    function claimToken(
        uint256 tranche,
        address receiver,
        address tokenAddress,
        uint256 airdropAmount,
        bytes memory signature,
        bytes32[] memory merkleProof
    ) public nonReentrant {
        require(tranche < _tranches, "Airdrop has not open this tranche");
        require(!_isPaused[tranche], "Aridrop is under maintaining mode in this tranche");

        bytes32 hashToSign = _hashAirdropToken(receiver, tokenAddress, airdropAmount);

        require(_verifyReceiver(receiver, tranche, hashToSign, signature), "Liquidity provider already claimed");
        require(_verifyClaim(tranche, hashToSign, merkleProof), "Incorrect merkle proof");

        _claimedOrFinalized[tranche][hashToSign] = true;

        _disburseToken(receiver, tokenAddress, airdropAmount);

        emit ClaimedToken(receiver, tranche, tokenAddress, airdropAmount);
    }

    function claimNft721(
        uint256 tranche,
        address receiver,
        address nftAddress,
        uint256[] memory tokenIds,
        bytes memory signature,
        bytes32[] memory merkleProof
    ) public nonReentrant {
        require(tranche < _tranches, "Airdrop has not open this tranche");
        require(!_isPaused[tranche], "Aridrop is under maintaining mode in this tranche");

        bytes32 hashToSign = _hashAirdropNFT721(receiver, nftAddress, tokenIds);

        require(_verifyReceiver(receiver, tranche, hashToSign, signature), "Liquidity provider already claimed");
        require(_verifyClaim(tranche, hashToSign, merkleProof), "Incorrect merkle proof");

        _claimedOrFinalized[tranche][hashToSign] = true;

        _disburseNFT721(receiver, nftAddress, tokenIds);

        emit ClaimNFT721(receiver, nftAddress, tokenIds);
    }

    function claimNft1155(
        uint256 tranche,
        address receiver,
        address nftAddress,
        uint256[] memory tokenIds,
        uint256[] memory amounts,
        bytes memory signature,
        bytes32[] memory merkleProof
    ) public nonReentrant {
        require(tranche < _tranches, "Airdrop has not open this tranche");
        require(!_isPaused[tranche], "Aridrop is under maintaining mode in this tranche");

        bytes32 hashToSign = _hashAirdropNFT1155(receiver, nftAddress, tokenIds, amounts);

        require(_verifyReceiver(receiver, tranche, hashToSign, signature), "Liquidity provider already claimed");
        require(_verifyClaim(tranche, hashToSign, merkleProof), "Incorrect merkle proof");

        _claimedOrFinalized[tranche][hashToSign] = true;

        _disburseNFT1155(receiver, nftAddress, tokenIds, amounts);

        emit ClaimNFT1155(receiver, nftAddress, tokenIds, amounts);
    }

    function dropNativeCoin(
        address[] memory receivers, 
        uint256[] memory amounts
    ) public nonReentrant selfExecute {
        require(receivers.length == amounts.length, "Invalid length of amounts");

        uint256 total = 0;
        for(uint256 j = 0; j < amounts.length; j++) {
            total = total + amounts[j];
        }
        require(address(this).balance >= total, "Not enough balance");

        for (uint256 i = 0; i < receivers.length; i++) {
            _disburseNativeCoin(receivers[i], amounts[i]);
        }
    }

    function dropToken(
        address[] memory receivers, 
        address tokenAddress,
        uint256[] memory amounts
    ) public nonReentrant selfExecute {
        require(receivers.length == amounts.length, "Invalid length of amounts");

        uint256 total = 0;
        for(uint256 j = 0; j < amounts.length; j++) {
            total = total + amounts[j];
        }
        require(IERC20Upgradeable(tokenAddress).balanceOf(address(this)) >= total, "Not enough balance");

        for (uint256 i = 0; i < receivers.length; i++) {
            _disburseToken(receivers[i], tokenAddress, amounts[i]);
        }
    }

    function dropNFT721(
        address[] memory receivers, 
        address nftAddress,
        uint256[][] memory tokenIdsPacks
    ) public nonReentrant selfExecute {
        require(receivers.length == tokenIdsPacks.length, "Invalid length of amounts");

        for (uint256 i = 0; i < receivers.length; i++) {
            _disburseNFT721(receivers[i], nftAddress, tokenIdsPacks[i]);
        }
    }

    function dropNFT1155(
        address[] memory receivers, 
        address nftAddress,
        uint256[][] memory tokenIdsPacks,
        uint256[][] memory amountsPacks
    ) public nonReentrant selfExecute {
        require(receivers.length == tokenIdsPacks.length, "Invalid length of amounts");

        for (uint256 i = 0; i < receivers.length; i++) {
            _disburseNFT1155(receivers[i], nftAddress, tokenIdsPacks[i], amountsPacks[i]);
        }
    }

    function _hashAirdropNativeCoin(
        address receiver,
        uint256 airdropAmount
    ) private view returns (bytes32) {
        return keccak256(abi.encodePacked(
            "\x19\x01",
            domainSeparator,
            keccak256(abi.encode(
                airdropNativeCoinTypeHash,
                keccak256(abi.encodePacked(receiver)),
                keccak256(abi.encodePacked(airdropAmount))
            ))
        ));
    }

    function _hashAirdropToken(
        address receiver,
        address tokenAddress,
        uint256 airdropAmount
    ) private view returns (bytes32) {
        return keccak256(abi.encodePacked(
            "\x19\x01",
            domainSeparator,
            keccak256(abi.encode(
                airdropTokenTypeHash,
                keccak256(abi.encodePacked(receiver)),
                keccak256(abi.encodePacked(tokenAddress)),
                keccak256(abi.encodePacked(airdropAmount))
            ))
        ));
    }

    function _hashAirdropNFT721(
        address receiver,
        address nftAddress,
        uint256[] memory tokenIds
    ) private view returns (bytes32) {
        return keccak256(abi.encodePacked(
            "\x19\x01",
            domainSeparator,
            keccak256(abi.encode(
                airdropNFT721TypeHash,
                keccak256(abi.encodePacked(receiver)),
                keccak256(abi.encodePacked(nftAddress)),
                keccak256(abi.encodePacked(tokenIds))
            ))
        ));
    }

    function _hashAirdropNFT1155(
        address receiver,
        address nftAddress,
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) private view returns (bytes32) {
        return keccak256(abi.encodePacked(
            "\x19\x01",
            domainSeparator,
            keccak256(abi.encode(
                airdropNFT1155TypeHash,
                keccak256(abi.encodePacked(receiver)),
                keccak256(abi.encodePacked(nftAddress)),
                keccak256(abi.encodePacked(tokenIds)),
                keccak256(abi.encodePacked(amounts))
            ))
        ));
    }

    function _verifyReceiver(
        address receiver,
        uint256 tranche,
        bytes32 hashToSign,
        bytes memory signature
    ) private view returns (bool valid) {
        if(_claimedOrFinalized[tranche][hashToSign]) {
            return false;
        }
        return ECDSAUpgradeable.recover(hashToSign, signature) == receiver;
    }

    function _verifyClaim(
        uint256 tranche,
        bytes32 leaf,
        bytes32[] memory merkleProof
    ) 
        private
        view
        returns (bool valid)
    {
        return MerkleProofUpgradeable.verify(merkleProof, _merkleRoots[tranche], keccak256(abi.encodePacked(leaf)));
    }

    function _disburseNativeCoin(
        address receiver, 
        uint256 airdropAmount
    ) private {
        require(receiver != address(0), "Invalid address");

        if(airdropAmount > 0) {
            payable(receiver).transfer(airdropAmount);
        } else {
            revert("No balance would be transferred");
        }
    }

    function _disburseToken(
        address receiver, 
        address tokenAddress, 
        uint256 airdropAmount
    ) private {
        require(receiver != address(0), "Invalid address");

        if(airdropAmount > 0) {
            IERC20Upgradeable(tokenAddress).safeTransfer(receiver, airdropAmount);
        } else {
            revert("No balance would be transferred");
        }
    }

    function _disburseNFT721(
        address receiver, 
        address nftAddress, 
        uint256[] memory tokenIds
    ) private {
        require(receiver != address(0), "Invalid address");

        if(tokenIds.length > 0) {
            for(uint256 i = 0; i < tokenIds.length; i++) {
                IERC721Upgradeable(nftAddress).safeTransferFrom(address(this), receiver, tokenIds[i]);
            }
        } else {
            revert("No token would be transferred");
        }
    }

    function _disburseNFT1155(
        address receiver, 
        address nftAddress, 
        uint256[] memory tokenIds, 
        uint256[] memory amounts
    ) private {
        require(receiver != address(0), "Invalid address");

        if(tokenIds.length > 0) {
            IERC1155Upgradeable(nftAddress).safeBatchTransferFrom(address(this), receiver, tokenIds, amounts, bytes(""));
        } else {
            revert("No token would be transferred");
        }
    }

    function emergencyWithdrawToken(
        address receiver, 
        address tokenAddress
    ) public selfExecute {
        _disburseNativeCoin(receiver, address(this).balance);
        _disburseToken(receiver, tokenAddress, IERC20Upgradeable(tokenAddress).balanceOf(address(this)));
    }

    function emergencyWithdrawNFT721(
        address receiver, 
        address nftAddress, 
        uint256[] memory tokenIds
    ) public selfExecute {
        _disburseNFT721(receiver, nftAddress, tokenIds);
    }

    function emergencyWithdrawNFT1155(
        address receiver, 
        address nftAddress, 
        uint256[] memory tokenIds,
        uint256[] memory amounts
    ) public selfExecute {
        _disburseNFT1155(receiver, nftAddress, tokenIds, amounts);
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override(AccessControlUpgradeable, ERC1155ReceiverUpgradeable) returns (bool) {
        return super.supportsInterface(interfaceId);
    }
    
    fallback() external payable {}
}
