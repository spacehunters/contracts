////// SPDX-License-Identifier-FLATTEN-SUPPRESS-WARNING: MIT
pragma solidity ^0.8.9;

////import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
////import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
////import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
////import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
////import "@openzeppelin/contracts-upgradeable/governance/TimelockControllerUpgradeable.sol"; 
////import "../assets/ERC1155ERC721.sol";
////import "../../common/chainlink/IRandomness.sol";

contract MysteryBoxSale is 
    Initializable, 
    TimelockControllerUpgradeable, 
    ReentrancyGuardUpgradeable
{
    using SafeERC20Upgradeable for IERC20Upgradeable;

    ERC1155ERC721 public asset;
    IERC20Upgradeable public exchangeToken;
    IRandomness public randomness;

    bytes private mysteryBoxes;
    uint256 public boxPrice;

    event OpenBox(address opener, uint256 id, uint256 supply, uint8 rarity);

    modifier selfExecute {
        require(_msgSender() == address(this), "Caller must be timelock");
        _;
    }

    function initialize(
        uint256 minDelay,
        address[] memory proposers,
        address[] memory executors,
        address _asset,
        address _exchangeToken,
        address _randomness,
        bytes memory _mysteryBoxes
    ) initializer public {
        __TimelockController_init(minDelay, proposers, executors);
        __ReentrancyGuard_init();

        asset = ERC1155ERC721(_asset);
        exchangeToken = IERC20Upgradeable(_exchangeToken);
        randomness = IRandomness(_randomness);
        mysteryBoxes = _mysteryBoxes;

        revokeRole(TIMELOCK_ADMIN_ROLE, _msgSender());
    }

    function getRandomBoxIndex(address opener) public returns (uint256 index) {
        require(mysteryBoxes.length > 0, "mystery box are empty");

        index = randomness.getRandomSeed(opener) % mysteryBoxes.length;
    }

    function setBoxPrice(uint256 _price) external selfExecute {
        boxPrice = _price;
    }

    function openBox(uint40 packId, bytes32 hash, bytes calldata data) 
        external 
        nonReentrant 
    {
        uint256 balance = exchangeToken.balanceOf(_msgSender());
        require(balance >= boxPrice, "Not enought balance");
        exchangeToken.safeTransferFrom(_msgSender(), address(this), boxPrice);

        uint256 index = getRandomBoxIndex(_msgSender());
        uint8 rarity = uint8(uint8(mysteryBoxes[index]) - 48);
        mysteryBoxes[index] = mysteryBoxes[mysteryBoxes.length - 1];
        mysteryBoxes.pop();

        uint256 id = asset.mint(
            _msgSender(),
            packId,
            hash,
            1,
            rarity,
            _msgSender(),
            data
        );

        emit OpenBox(opener, id, supply, rarity);
    }

    function emergencyWithdraw(address recipient, uint256 amount) external selfExecute {
        require(recipient != address(0), "Invalid address");
        require(amount > 0, "Amount is Zero");

        // Transfer native coin
        payable(recipient).transfer(amount);
        // Transfer token
        exchangeToken.safeTransfer(recipient, amount);
    }
}
